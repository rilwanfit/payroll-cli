{
    "name": "rilwan/rtl-mikko-test",
    "description": "A small command-line utility to help a fictional company determine the dates they need to pay salaries to their sales department",
    "type": "project",
    "require": {
        "symfony/console": "^5.2",
        "league/csv": "^9.7",
        "nesbot/carbon": "^2.46",
        "monolog/monolog": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.83",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Mohamed Hanifa Rilwan",
            "email": "rilwanfit@gmail.com"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Functional\\Test\\": "tests/functional",
            "Stubs\\": "tests/stubs",
            "Unit\\Test\\": "tests/unit"
        }
    }
}
